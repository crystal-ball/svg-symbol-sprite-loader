{
  "name": "svg-symbol-sprite-loader",
  "version": "0.0.0-semantic",
  "description": "Loader and plugin for generating an SVG symbol sprite",
  "main": "src/index.js",
  "browser": "src/icon-sprite-loader.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/svg-symbol-sprite-loader.git"
  },
  "keywords": [
    "webpack",
    "svg",
    "loader",
    "sprite"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "commit": "git cz",
    "test": "npm run lint && jest",
    "container": "docker-compose run --service-ports --rm test-app",
    "test:unit": "jest",
    "format": "prettier --write \"{src,test-app}/*.js\"",
    "lint": "cross-env NODE_ENV=test eslint src/*"
  },
  "peerDependencies": {
    "webpack": "4.x"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-customizable": "^6.0.0",
    "eslint-config-eloquence": "^10.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.16.0",
    "semantic-release": "^15.13.3",
    "webpack": "^4.29.0"
  },
  "husky": {
    "prepush": "npm run test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
