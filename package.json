{
  "name": "svg-symbol-sprite-loader",
  "version": "0.4.0",
  "description": "Loader and plugin for generating an SVG symbol sprite",
  "main": "src/loader.js",
  "scripts": {
    "commit": "git cz",
    "test": "npm run lint && jest",
    "prepush": "npm test",
    "container": "docker-compose run --service-ports --rm test-app",
    "test:unit": "jest",
    "format": "prettier --write src/*.js",
    "lint": "cross-env NODE_ENV=test eslint src/*"
  },
  "keywords": [
    "webpack",
    "svg",
    "loader",
    "sprite"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/svg-symbol-sprite-loader.git"
  },
  "license": "ISC",
  "peerDependencies": {
    "webpack": "^3 || ^4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "loader-utils": "^1.1.0",
    "webpack-sources": "^1.1.0"
  },
  "devDependencies": {
    "@crystal-ball/eslint-config-eloquence": "^6.3.0",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.4",
    "cz-customizable": "^5.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "memory-fs": "^0.4.1",
    "prettier": "^1.11.1",
    "webpack": "^4.1.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
