{
  "name": "svg-symbol-sprite-loader",
  "version": "5.1.0",
  "description": "Loader and plugin for generating an SVG symbol sprite",
  "main": "src/index.js",
  "browser": "src/browser/icon-sprite-loader.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/svg-symbol-sprite-loader.git"
  },
  "keywords": [
    "webpack",
    "svg",
    "loader",
    "sprite"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {
    "commit": "cz",
    "container": "docker-compose run --service-ports --rm test-app",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint src",
    "test:unit": "jest",
    "format": "prettier --write '*.{js,json,md}' '**/*.{js,json,md,mdx,scss}' !CHANGELOG.md"
  },
  "dependencies": {
    "loader-utils": "2",
    "parse5": "6",
    "safe-require": "1",
    "webpack-sources": "2"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "2.11.0",
    "@crystal-ball/prettier-base": "2.9.0",
    "@crystal-ball/semantic-release-base": "2.34.0",
    "@types/jest": "27.0.3",
    "babel-preset-env": "1.7.0",
    "eslint-config-eloquence": "20.0.2",
    "jest": "27.4.2",
    "memfs": "3.4.0",
    "webpack": "5.64.4"
  }
}
